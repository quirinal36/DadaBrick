<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="token_sql">
	<insert id="insert" parameterType="token">
		INSERT INTO Tokens
		(
			userId,
			token,
			isPwd
		)
		values
		(
			${userId},
			'${token}',
			${isPwd}
		)
	</insert>
</mapper>